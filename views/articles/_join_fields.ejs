<!--user객체의 id 값 필수-->
<!--req.body에서 가져올 name 값인 userid,email,pw 설정-->
<div>
    <input required value="<%=user.userid%>" name="userid" class ="user-id" placeholder="id를 입력하세요">
</div>
<div>
    <input required value="<%=user.email%>" name="email" class ="user-email" placeholder="email을 입력하세요">
</div>
<!--user객체의 pw 값 필수-->
<div>
    <input required value="<%=user.pw%>" name="pw" class ="user-pw" placeholder="pw를 입력하세요">
</div>
<button type="submit">회원가입 완료</button>